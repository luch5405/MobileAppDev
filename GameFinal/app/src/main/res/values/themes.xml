<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GameFinal" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/md_theme_light_primary</item>
        <item name="colorPrimaryVariant">@color/md_theme_light_onPrimaryContainer</item>
        <item name="colorOnPrimary">@color/md_theme_light_onPrimary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/md_theme_light_secondary</item>
        <item name="colorSecondaryVariant">@color/md_theme_light_secondaryContainer</item>
        <item name="colorOnSecondary">@color/md_theme_light_onSecondary</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="buttonStyle">@color/button</item>
    </style>
    <style name="Theme.GameFinal.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.GameFinal.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.GameFinal.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>